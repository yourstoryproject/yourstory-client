{
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "name": "yourstoryproject-client",
  "version": "0.0.1",
  "description": "Web client for YourStoryProject",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "format": "prettier-eslint --write 'src/js/**/*.js' && eslint --fix 'src/js/**/*.js'",
    "start": "webpack-dev-server --config webpack.dev.js",
    "serve": "firebase serve",
    "test": "echo \"This is a test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourstoryproject/yourstoryproject-client.git"
  },
  "author": "Joshua Wu",
  "license": "SEE LICENSE IN LICENSE.MD",
  "bugs": {
    "url": "https://github.com/yourstoryproject/yourstoryproject-client/issues"
  },
  "homepage": "https://github.com/yourstoryproject/yourstoryproject-client#readme",
  "dependencies": {
    "firebase": "^6.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-firebaseui": "^4.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-sort-destructure-keys": "^1.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
