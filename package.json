{
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "corejs": "3.0.0",
          "useBuiltIns": "entry"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "name": "yourstoryproject-client",
  "version": "0.0.1",
  "description": "Web client for YourStoryProject",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "format": "prettier-eslint --write 'src/js/**/*.+(js|ts)?(x)' && eslint --fix 'src/js/**/*.+(js|ts)?(x)'",
    "lint": "prettier-eslint --list-different 'src/js/**/*.+(js|ts)?(x)' && eslint 'src/js/**/*.+(js|ts)?(x)'",
    "start": "webpack-dev-server --config webpack.dev.js",
    "serve": "firebase serve",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourstoryproject/yourstoryproject-client.git"
  },
  "author": "Joshua Wu",
  "license": "SEE LICENSE IN LICENSE.MD",
  "bugs": {
    "url": "https://github.com/yourstoryproject/yourstoryproject-client/issues"
  },
  "homepage": "https://github.com/yourstoryproject/yourstoryproject-client#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "firebase": "^6.3.1",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-firebaseui": "^4.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/autobind-decorator": "^2.1.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/moment": "^2.13.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.5",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-sort-destructure-keys": "^1.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "file-loader": "^4.2.0",
    "firebase-mock": "^2.2.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
